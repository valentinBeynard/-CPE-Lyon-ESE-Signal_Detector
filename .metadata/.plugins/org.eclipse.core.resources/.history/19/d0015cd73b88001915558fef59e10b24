#ifndef SIGNAL_HANDLER_H_
#define SIGNAL_HANDLER_H_

#define SAMPLES_NBR	512

typedef struct
{
	uint8_t sampling_buff[SAMPLES_NBR];
	uint8_t is_active;
}SAMPLES_PCK;

void crosscorrelation(uint8_t* ref_signal, uint8_t* comp_signal);


#endif /* SIGNAL_HANDLER_H_ */
