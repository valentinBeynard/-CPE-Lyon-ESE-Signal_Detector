uint8_t ADC_value,reference[M_signal_pts],comparaison[M_signal_pts];

float y[M_signal_pts + M_signal_pts - 1];
uint8_t corr_func[M_signal_pts + M_signal_pts - 1];
uint16_t corr_max_abs = 0;
uint16_t max;
int l;
short detection=1;
